        "icon": "./src/renderer/img/icones/setelogosmall.png"
      },
      "electronInstallerRedhat": {
        "icon": "./src/renderer/img/icones/setelogosmall.png"
      },
      "github_repository": {
        "owner": "",
        "name": ""
